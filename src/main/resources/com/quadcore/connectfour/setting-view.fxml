<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #202020;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.quadcore.connectfour.view.StartController">
   <children>
      <ImageView fitHeight="104.0" fitWidth="347.0" layoutX="127.0" layoutY="189.0" onMouseClicked="#noPruningButtonClicked" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@nopruningBtn.png" />
         </image>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </ImageView>
      <ImageView fitHeight="90.0" fitWidth="347.0" layoutX="145.0" layoutY="261.0" onMouseClicked="#withPruningButtonClicked" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@pruningBtn.png" />
         </image>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </ImageView>
      <Label layoutX="115.0" layoutY="34.0" prefHeight="51.0" prefWidth="376.0" text="Enter number of levels in minmax tree" textFill="WHITE">
         <font>
            <Font name="Dubai Regular" size="24.0" />
         </font>
      </Label>
      <Label fx:id="errorLabel" layoutX="318.0" layoutY="93.0" prefHeight="32.0" prefWidth="263.0" text="Label" textFill="#ff9c9c" visible="false">
         <font>
            <Font name="Dubai Light" size="18.0" />
         </font>
      </Label>
      <TextField fx:id="levelTxt" layoutX="115.0" layoutY="93.0" />
   </children>
</AnchorPane>
